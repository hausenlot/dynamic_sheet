<turbo-frame id="table_contents">
   <table>
    <tbody>
      <% @records.each do |record| %>
        <tr>
          <% @remaining_columns.each do |col| %>
            <td><%= record.send(col) %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to "Download Users Report", main_excel_path(format: :xlsx, model: params[:model], remaining_columns: @remaining_columns) %>
</turbo-frame>